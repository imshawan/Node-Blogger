<table class="table table-hover table-striped">
    <thead>
      <tr>
        <th class="font-weight-bolder">Username</th>
        <th class="font-weight-bolder">Email</th>
      </tr>
    </thead>
    
    <tbody>

      <% if (users && users.length) { %>
        <% users.forEach(element => { %>
          <tr>
            <td><%= element.fullname || element.username %></td>
            <td><%= element.email %></td>
          </tr>
        <% }); %>
    <% } %>


      
    </tbody>
  </table>