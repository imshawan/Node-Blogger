{
    "siteName": "Node Blogger",
    "siteNameUrl": "",
    "siteShortName": "Node Blogger",
    "description": "A simple open-source blogging application powered by MongoDB & NodeJS",
    "keywords": [],
    "logo": "",
    "favicon": "",
    "altLogoText": "",
    "logoRedirectionUrl": "",
    "session": {
        "name": "session.sid",
        "saveUninitialized": false,
        "resave": false
    },
    "cookie": {
        "maxTTLDays": 14
    },
    "cors": {
        "allowedHeaders": "Origin, X-Requested-With, Cookie, Content-Type, Accept, X-Access-Token, Authorization",
        "whitelistOrigins": "*",
        "credentials": true
    },
    "manifest": {
        "themeColor": "#ffffff",
        "backgroundColor": "#ffffff"
    },
    "robots": {
        "allowed": ["/"],
        "disallowed": [],
        "userAgents": []
    },
    "xPoweredByHeaders": "NodeBlogger",
    "maximumRequestBodySize": "50mb",
    "allowTags": true,
    "allowLike": true,
    "allowDislike": true,
    "allowComments": true,
    "allowGuestComments": true,
    "allowUsernameChange": false,
    "allowEmailChange": true,
    "allowAccountDeletion": true,
    "allowSelfSuspension": false,
    "gdprConsent": true,
    "storeUsernameHistory": true,
    "automaticLogoutDuration": 0,
    "accountLockDuration": 120,
    "sessionExpiryDuration": "14",
    "maxLoginPerHour": 20,
    "maxPasswordResetRequests": 10,
    "minPostTitleLength": 2,
    "maxPostTitleLength": 250,
    "minPostLength": 250,
    "maxPostLength": 2550,
    "minTagsPerPost": 0,
    "maxTagsPerPost": 10,
    "maxPostThumbnailSize": "5mb",
    "allowedFileTypes": ["jpg", "png", "jpeg", "gif", "bmp", "txt"],
    "maxFileSize": "50mb",
    "externalLinks": "_blank",
    "registerationType": "normal",
    "maxPasswordLength": 128,
    "maxUsernameLength": 128,
    "maxEmailLength": 128,
    "maxCategoryNameLength": 50,
    "maxCategoryDescriptionLength": 500,
    "maxCategoryBlurbLength": 200,
    "minPasswordLength": 6,
    "minUsernameLength": 4,
    "minEmailLength": 8,
    "minPasswordStrength": 2,
    "maxFullnameLength": 30,
    "minFullnameLength": 5,
    "maxLocationLength": 100,
    "minLocationLength": 10,
    "maxBioLength": 250,
    "minBioLength": 50,
    "maxAboutLength": 1000,
    "minAboutLength": 200,
    "maxTagLength": 30,
    "minTagLength": 3,
    "sorting": "default",
    "registrationType": "default"
}