{
    "siteName": "Node Blogger",
    "siteNameUrl": "",
    "siteShortName": "Node Blogger",
    "description": "A simple open-source blogging application powered by MongoDB & NodeJS",
    "keywords": [],
    "logo": "",
    "favicon": "",
    "altLogoText": "",
    "logoRedirectionUrl": "",
    "session": {
        "name": "session.sid",
        "saveUninitialized": false,
        "resave": false
    },
    "cookie": {
        "maxTTLDays": 14
    },
    "cors": {
        "allowedHeaders": "Origin, X-Requested-With, Cookie, Content-Type, Accept, X-Access-Token, Authorization",
        "whitelistOrigins": "*",
        "credentials": true
    },
    "manifest": {
        "themeColor": "#ffffff",
        "backgroundColor": "#ffffff"
    },
    "robots": {
        "allowed": ["/"],
        "disallowed": [],
        "userAgents": []
    },
    "xPoweredByHeaders": "NodeBlogger",
    "maximumRequestBodySize": "50mb",
    "maxPasswordLength": 128,
    "maxUsernameLength": 128,
    "maxEmailLength": 128,
    "maxCategoryBlurbLength": 200,
    "minPasswordLength": 6,
    "minUsernameLength": 4,
    "minEmailLength": 8,
    "minPasswordStrength": 2,
    "maxFullnameLength": 30,
    "minFullnameLength": 5,
    "maxLocationLength": 100,
    "minLocationLength": 10,
    "maxBioLength": 250,
    "minBioLength": 50,
    "maxAboutLength": 1000,
    "minAboutLength": 200,
    "maxTagLength": 30,
    "minTagLength": 3
}